{
    "name": "browser-basedpyright",
    "displayName": "basedpyright (browser)",
    "description": "Browser build of basedpyright",
    "//": "version gets set in the ci when publishing",
    "version": "0.0.0",
    "license": "MIT",
    "author": {
        "name": "detachhead"
    },
    "publisher": "detachhead",
    "engines": {
        "node": ">=12.0.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/detachhead/basedpyright",
        "directory": "packages/browser-pyright"
    },
    "scripts": {
        "build": "webpack --mode production --progress",
        "clean": "shx rm -rf ./dist ./out README.md LICENSE.txt",
        "prepack": "npm run clean && shx cp ../../README.md . && shx cp ../../LICENSE.txt . && npm run build",
        "webpack": "webpack --mode development --progress"
    },
    "devDependencies": {
        "@types/copy-webpack-plugin": "^8.0.1",
        "@types/node": "^17.0.45",
        "copy-webpack-plugin": "^11.0.0",
        "shx": "^0.3.4",
        "ts-loader": "^9.5.1",
        "typescript": "~5.5.0-beta",
        "webpack": "^5.90.1",
        "webpack-cli": "^5.1.4"
    },
    "files": [
        "/dist",
        "LICENSE.txt"
    ],
    "main": "index.js",
    "dependencies": {
        "buffer": "^6.0.3",
        "path-browserify": "^1.0.1",
        "vscode-languageserver": "^10.0.0-next.6",
        "crypto-browserify": "^3.12.0",
        "os-browserify": "^0.3.0",
        "stream-browserify": "^3.0.0",
        "util": "^0.12.5",
        "vm-browserify": "^1.1.2"
    }
}
